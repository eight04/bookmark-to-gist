{
  "manifest_version": 2,
  "homepage_url": "https://github.com/eight04/bookmark-to-gist",
  "name": "bookmark-to-gist",
  "version": "1.0.0",
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "images/icon.svg",
    "48": "images/icon.svg"
  },
  "background": {
    "scripts": [
    ],
    "persistent": true
  },
  "options_ui": {
    "open_in_tab": false,
    "page": "options.html"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "alarms"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "bookmark-to-gist@eight04.blogspot.com"
    }
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon.svg",
      "48": "images/icon.svg"
    },
    "default_title": "__MSG_extName__"
  }
}
